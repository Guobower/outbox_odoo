<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
	<data>
		<record model="ir.ui.view" id="grupo_atendimento_tree">
			<field name="name">grupo_atendimento.tree</field>
			<field name="model">grupo_atendimento</field>
			<field name="view_type">tree</field>
			<field name="priority" eval="16" />
			<field name="arch" type="xml">
				<!-- view content: <form>, <tree>, <graph>, ... -->
				<tree string="Grupo de Atendimento" default_order='name, manifestacao_atendimento asc'>
					<field name="name" />
					<field name="manifestacao_atendimento" />
					<field name="descricao" />
				</tree>
			</field>
		</record>
	</data>
</openerp>