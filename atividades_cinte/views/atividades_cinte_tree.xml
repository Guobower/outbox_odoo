<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
	<data>
		<record model="ir.ui.view" id="atividades_cinte_tree">
			<field name="name">atividades_cinte.tree</field>
			<field name="model">atividades_cinte</field>
			<field name="view_type">tree</field>
			<field name="priority" eval="16" />
			<field name="arch" type="xml">
				<!-- view content: <form>, <tree>, <graph>, ... -->
				<tree string="Atividades">
					<field name="name" />
					<field name="responsavel" />
					<field name="atividade" />
					<field name="dataPrevisao" />
					<field name="dataConclusao" />
					<field name="equipe" widget="many2many_tags" />
					<field name="stage_id" />
				</tree>
			</field>
		</record>
	</data>
</openerp>